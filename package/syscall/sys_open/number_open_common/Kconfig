	default "https://gitee.com/BiscuitOS_team/HardStack/raw/Gitee/SystemCall/sys_open/number_open_common"
	string "url"

config PK_SYS_OPEN_NUMBER_GITHUB
	string "GitHub"

config PK_SYS_OPEN_NUMBER_SRCFILE
	default "main.c Makefile"
	string "Source code list"

config PK_SYS_OPEN_NUMBER_CSRCFILE
	string "Compile source code list"

config PK_SYS_OPEN_NUMBER_CONFIG
	string "CONFIG: Project Configure Options"

config PK_SYS_OPEN_NUMBER_LIBPATH
	string "LIBS: Libraries to pass to the linker, e.g. -l<library>"

config PK_SYS_OPEN_NUMBER_CPPFLAGS
	string "CPPFLAGS: C/C++ preprocessor flags, e.g. -I<include dir>"

config PK_SYS_OPEN_NUMBER_DPKCONFIG
	string "PKG_CONFIG_PATH: Directories to add to pkg-config's search path"

config PK_SYS_OPEN_NUMBER_CFLAGS
	string "CFLAGS: C compiler flags"

config PK_SYS_OPEN_NUMBER_LDFLAGS
	string "LDFLAGS: Linker flags"

config PK_SYS_OPEN_NUMBER_CXXFLAGS
	string "CXXFLAGS: C++ compiler flags"

config PK_SYS_OPEN_NUMBER_ASFLAGS
	string "CCASFLAGS: assembler compiler flags (defaults to CFLAGS)"

config PK_SYS_OPEN_NUMBER_BUILDARCH
	string "--host options on configure for Architecture"

