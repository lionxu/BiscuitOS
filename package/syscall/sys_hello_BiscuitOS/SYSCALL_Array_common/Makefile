################################################
#
# SYSCALL_Array_common
#
################################################
PK_SYS_SYSCALL_ARRAY_NAME        := SYSCALL_Array_common
PK_SYS_SYSCALL_ARRAY_VERSION     := $(CONFIG_PK_SYS_SYSCALL_ARRAY_VERSION)
PK_SYS_SYSCALL_ARRAY_SITE        := $(CONFIG_PK_SYS_SYSCALL_ARRAY_URL)
PK_SYS_SYSCALL_ARRAY_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_ARRAY_GITHUB)
PK_SYS_SYSCALL_ARRAY_SUBNAME     :=
PK_SYS_SYSCALL_ARRAY_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_ARRAY_BUILDARCH)
PK_SYS_SYSCALL_ARRAY_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_Array_common
PK_SYS_SYSCALL_ARRAY_PATCH       := $(PK_SYS_SYSCALL_ARRAY_DIR)/patch
PK_SYS_SYSCALL_ARRAY_BSBIT       := SYSCALL_Array_common-$(PK_SYS_SYSCALL_ARRAY_VERSION).bsbit
PK_SYS_SYSCALL_ARRAY_OUT         :=
PK_SYS_SYSCALL_ARRAY_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_ARRAY_TAR         := $(CONFIG_PK_SYS_SYSCALL_ARRAY_TAR)
PK_SYS_SYSCALL_ARRAY_PACKAGE     :=
PK_SYS_SYSCALL_ARRAY_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_ARRAY_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_ARRAY_DIR)/build

PK_SYS_SYSCALL_ARRAY_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_ARRAY_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_ARRAY_NAME)X \
                 $(PK_SYS_SYSCALL_ARRAY_VERSION)X \
                 $(PK_SYS_SYSCALL_ARRAY_SITE)X \
                 $(PK_SYS_SYSCALL_ARRAY_GITHUB)X \
                 $(PK_SYS_SYSCALL_ARRAY_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_ARRAY_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_ARRAY_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_ARRAY_BSBIT)X \
                 $(PK_SYS_SYSCALL_ARRAY_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_ARRAY_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_ARRAY_CSRCFILE)

$(PK_SYS_SYSCALL_ARRAY_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_ARRAY_sh,$(srctree))
