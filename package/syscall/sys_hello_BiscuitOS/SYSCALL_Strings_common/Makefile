################################################
#
# SYSCALL_Strings_common
#
################################################
PK_SYS_SYSCALL_STRINGS_NAME        := SYSCALL_Strings_common
PK_SYS_SYSCALL_STRINGS_VERSION     := $(CONFIG_PK_SYS_SYSCALL_STRINGS_VERSION)
PK_SYS_SYSCALL_STRINGS_SITE        := $(CONFIG_PK_SYS_SYSCALL_STRINGS_URL)
PK_SYS_SYSCALL_STRINGS_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_STRINGS_GITHUB)
PK_SYS_SYSCALL_STRINGS_SUBNAME     :=
PK_SYS_SYSCALL_STRINGS_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_STRINGS_BUILDARCH)
PK_SYS_SYSCALL_STRINGS_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_Strings_common
PK_SYS_SYSCALL_STRINGS_PATCH       := $(PK_SYS_SYSCALL_STRINGS_DIR)/patch
PK_SYS_SYSCALL_STRINGS_BSBIT       := SYSCALL_Strings_common-$(PK_SYS_SYSCALL_STRINGS_VERSION).bsbit
PK_SYS_SYSCALL_STRINGS_OUT         :=
PK_SYS_SYSCALL_STRINGS_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_STRINGS_TAR         := $(CONFIG_PK_SYS_SYSCALL_STRINGS_TAR)
PK_SYS_SYSCALL_STRINGS_PACKAGE     :=
PK_SYS_SYSCALL_STRINGS_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_STRINGS_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_STRINGS_DIR)/build

PK_SYS_SYSCALL_STRINGS_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_STRINGS_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_STRINGS_NAME)X \
                 $(PK_SYS_SYSCALL_STRINGS_VERSION)X \
                 $(PK_SYS_SYSCALL_STRINGS_SITE)X \
                 $(PK_SYS_SYSCALL_STRINGS_GITHUB)X \
                 $(PK_SYS_SYSCALL_STRINGS_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_STRINGS_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_STRINGS_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_STRINGS_BSBIT)X \
                 $(PK_SYS_SYSCALL_STRINGS_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_STRINGS_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_STRINGS_CSRCFILE)

$(PK_SYS_SYSCALL_STRINGS_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_STRINGS_sh,$(srctree))
