################################################
#
# SYSCALL_DEFINE3_common
#
################################################
PK_SYS_SYSCALL_DEFINE3_NAME        := SYSCALL_DEFINE3_common
PK_SYS_SYSCALL_DEFINE3_VERSION     := $(CONFIG_PK_SYS_SYSCALL_DEFINE3_VERSION)
PK_SYS_SYSCALL_DEFINE3_SITE        := $(CONFIG_PK_SYS_SYSCALL_DEFINE3_URL)
PK_SYS_SYSCALL_DEFINE3_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_DEFINE3_GITHUB)
PK_SYS_SYSCALL_DEFINE3_SUBNAME     :=
PK_SYS_SYSCALL_DEFINE3_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_DEFINE3_BUILDARCH)
PK_SYS_SYSCALL_DEFINE3_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_DEFINE3_common
PK_SYS_SYSCALL_DEFINE3_PATCH       := $(PK_SYS_SYSCALL_DEFINE3_DIR)/patch
PK_SYS_SYSCALL_DEFINE3_BSBIT       := SYSCALL_DEFINE3_common-$(PK_SYS_SYSCALL_DEFINE3_VERSION).bsbit
PK_SYS_SYSCALL_DEFINE3_OUT         :=
PK_SYS_SYSCALL_DEFINE3_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_DEFINE3_TAR         := $(CONFIG_PK_SYS_SYSCALL_DEFINE3_TAR)
PK_SYS_SYSCALL_DEFINE3_PACKAGE     :=
PK_SYS_SYSCALL_DEFINE3_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_DEFINE3_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_DEFINE3_DIR)/build

PK_SYS_SYSCALL_DEFINE3_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_DEFINE3_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_DEFINE3_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE3_VERSION)X \
                 $(PK_SYS_SYSCALL_DEFINE3_SITE)X \
                 $(PK_SYS_SYSCALL_DEFINE3_GITHUB)X \
                 $(PK_SYS_SYSCALL_DEFINE3_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE3_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_DEFINE3_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_DEFINE3_BSBIT)X \
                 $(PK_SYS_SYSCALL_DEFINE3_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_DEFINE3_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE3_CSRCFILE)

$(PK_SYS_SYSCALL_DEFINE3_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_DEFINE3_sh,$(srctree))
