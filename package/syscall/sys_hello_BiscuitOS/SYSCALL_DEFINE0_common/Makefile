################################################
#
# SYSCALL_DEFINE0_common
#
################################################
PK_SYS_SYSCALL_DEFINE0_NAME        := SYSCALL_DEFINE0_common
PK_SYS_SYSCALL_DEFINE0_VERSION     := $(CONFIG_PK_SYS_SYSCALL_DEFINE0_VERSION)
PK_SYS_SYSCALL_DEFINE0_SITE        := $(CONFIG_PK_SYS_SYSCALL_DEFINE0_URL)
PK_SYS_SYSCALL_DEFINE0_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_DEFINE0_GITHUB)
PK_SYS_SYSCALL_DEFINE0_SUBNAME     :=
PK_SYS_SYSCALL_DEFINE0_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_DEFINE0_BUILDARCH)
PK_SYS_SYSCALL_DEFINE0_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_DEFINE0_common
PK_SYS_SYSCALL_DEFINE0_PATCH       := $(PK_SYS_SYSCALL_DEFINE0_DIR)/patch
PK_SYS_SYSCALL_DEFINE0_BSBIT       := SYSCALL_DEFINE0_common-$(PK_SYS_SYSCALL_DEFINE0_VERSION).bsbit
PK_SYS_SYSCALL_DEFINE0_OUT         :=
PK_SYS_SYSCALL_DEFINE0_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_DEFINE0_TAR         := $(CONFIG_PK_SYS_SYSCALL_DEFINE0_TAR)
PK_SYS_SYSCALL_DEFINE0_PACKAGE     :=
PK_SYS_SYSCALL_DEFINE0_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_DEFINE0_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_DEFINE0_DIR)/build

PK_SYS_SYSCALL_DEFINE0_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_DEFINE0_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_DEFINE0_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE0_VERSION)X \
                 $(PK_SYS_SYSCALL_DEFINE0_SITE)X \
                 $(PK_SYS_SYSCALL_DEFINE0_GITHUB)X \
                 $(PK_SYS_SYSCALL_DEFINE0_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE0_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_DEFINE0_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_DEFINE0_BSBIT)X \
                 $(PK_SYS_SYSCALL_DEFINE0_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_DEFINE0_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE0_CSRCFILE)

$(PK_SYS_SYSCALL_DEFINE0_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_DEFINE0_sh,$(srctree))
