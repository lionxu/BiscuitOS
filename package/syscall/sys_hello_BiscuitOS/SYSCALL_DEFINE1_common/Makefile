################################################
#
# SYSCALL_DEFINE1_common
#
################################################
PK_SYS_SYSCALL_DEFINE1_NAME        := SYSCALL_DEFINE1_common
PK_SYS_SYSCALL_DEFINE1_VERSION     := $(CONFIG_PK_SYS_SYSCALL_DEFINE1_VERSION)
PK_SYS_SYSCALL_DEFINE1_SITE        := $(CONFIG_PK_SYS_SYSCALL_DEFINE1_URL)
PK_SYS_SYSCALL_DEFINE1_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_DEFINE1_GITHUB)
PK_SYS_SYSCALL_DEFINE1_SUBNAME     :=
PK_SYS_SYSCALL_DEFINE1_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_DEFINE1_BUILDARCH)
PK_SYS_SYSCALL_DEFINE1_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_DEFINE1_common
PK_SYS_SYSCALL_DEFINE1_PATCH       := $(PK_SYS_SYSCALL_DEFINE1_DIR)/patch
PK_SYS_SYSCALL_DEFINE1_BSBIT       := SYSCALL_DEFINE1_common-$(PK_SYS_SYSCALL_DEFINE1_VERSION).bsbit
PK_SYS_SYSCALL_DEFINE1_OUT         :=
PK_SYS_SYSCALL_DEFINE1_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_DEFINE1_TAR         := $(CONFIG_PK_SYS_SYSCALL_DEFINE1_TAR)
PK_SYS_SYSCALL_DEFINE1_PACKAGE     :=
PK_SYS_SYSCALL_DEFINE1_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_DEFINE1_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_DEFINE1_DIR)/build

PK_SYS_SYSCALL_DEFINE1_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_DEFINE1_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_DEFINE1_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE1_VERSION)X \
                 $(PK_SYS_SYSCALL_DEFINE1_SITE)X \
                 $(PK_SYS_SYSCALL_DEFINE1_GITHUB)X \
                 $(PK_SYS_SYSCALL_DEFINE1_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE1_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_DEFINE1_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_DEFINE1_BSBIT)X \
                 $(PK_SYS_SYSCALL_DEFINE1_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_DEFINE1_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE1_CSRCFILE)

$(PK_SYS_SYSCALL_DEFINE1_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_DEFINE1_sh,$(srctree))
