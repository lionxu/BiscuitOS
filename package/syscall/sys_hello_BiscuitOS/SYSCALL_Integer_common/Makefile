################################################
#
# SYSCALL_Integer_common
#
################################################
PK_SYS_SYSCALL_INTEGER_NAME        := SYSCALL_Integer_common
PK_SYS_SYSCALL_INTEGER_VERSION     := $(CONFIG_PK_SYS_SYSCALL_INTEGER_VERSION)
PK_SYS_SYSCALL_INTEGER_SITE        := $(CONFIG_PK_SYS_SYSCALL_INTEGER_URL)
PK_SYS_SYSCALL_INTEGER_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_INTEGER_GITHUB)
PK_SYS_SYSCALL_INTEGER_SUBNAME     :=
PK_SYS_SYSCALL_INTEGER_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_INTEGER_BUILDARCH)
PK_SYS_SYSCALL_INTEGER_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_Integer_common
PK_SYS_SYSCALL_INTEGER_PATCH       := $(PK_SYS_SYSCALL_INTEGER_DIR)/patch
PK_SYS_SYSCALL_INTEGER_BSBIT       := SYSCALL_Integer_common-$(PK_SYS_SYSCALL_INTEGER_VERSION).bsbit
PK_SYS_SYSCALL_INTEGER_OUT         :=
PK_SYS_SYSCALL_INTEGER_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_INTEGER_TAR         := $(CONFIG_PK_SYS_SYSCALL_INTEGER_TAR)
PK_SYS_SYSCALL_INTEGER_PACKAGE     :=
PK_SYS_SYSCALL_INTEGER_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_INTEGER_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_INTEGER_DIR)/build

PK_SYS_SYSCALL_INTEGER_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_INTEGER_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_INTEGER_NAME)X \
                 $(PK_SYS_SYSCALL_INTEGER_VERSION)X \
                 $(PK_SYS_SYSCALL_INTEGER_SITE)X \
                 $(PK_SYS_SYSCALL_INTEGER_GITHUB)X \
                 $(PK_SYS_SYSCALL_INTEGER_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_INTEGER_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_INTEGER_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_INTEGER_BSBIT)X \
                 $(PK_SYS_SYSCALL_INTEGER_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_INTEGER_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_INTEGER_CSRCFILE)

$(PK_SYS_SYSCALL_INTEGER_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_INTEGER_sh,$(srctree))
