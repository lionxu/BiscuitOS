################################################
#
# SYSCALL_DEFINE6_common
#
################################################
PK_SYS_SYSCALL_DEFINE6_NAME        := SYSCALL_DEFINE6_common
PK_SYS_SYSCALL_DEFINE6_VERSION     := $(CONFIG_PK_SYS_SYSCALL_DEFINE6_VERSION)
PK_SYS_SYSCALL_DEFINE6_SITE        := $(CONFIG_PK_SYS_SYSCALL_DEFINE6_URL)
PK_SYS_SYSCALL_DEFINE6_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_DEFINE6_GITHUB)
PK_SYS_SYSCALL_DEFINE6_SUBNAME     :=
PK_SYS_SYSCALL_DEFINE6_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_DEFINE6_BUILDARCH)
PK_SYS_SYSCALL_DEFINE6_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_DEFINE6_common
PK_SYS_SYSCALL_DEFINE6_PATCH       := $(PK_SYS_SYSCALL_DEFINE6_DIR)/patch
PK_SYS_SYSCALL_DEFINE6_BSBIT       := SYSCALL_DEFINE6_common-$(PK_SYS_SYSCALL_DEFINE6_VERSION).bsbit
PK_SYS_SYSCALL_DEFINE6_OUT         :=
PK_SYS_SYSCALL_DEFINE6_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_DEFINE6_TAR         := $(CONFIG_PK_SYS_SYSCALL_DEFINE6_TAR)
PK_SYS_SYSCALL_DEFINE6_PACKAGE     :=
PK_SYS_SYSCALL_DEFINE6_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_DEFINE6_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_DEFINE6_DIR)/build

PK_SYS_SYSCALL_DEFINE6_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_DEFINE6_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_DEFINE6_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE6_VERSION)X \
                 $(PK_SYS_SYSCALL_DEFINE6_SITE)X \
                 $(PK_SYS_SYSCALL_DEFINE6_GITHUB)X \
                 $(PK_SYS_SYSCALL_DEFINE6_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE6_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_DEFINE6_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_DEFINE6_BSBIT)X \
                 $(PK_SYS_SYSCALL_DEFINE6_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_DEFINE6_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE6_CSRCFILE)

$(PK_SYS_SYSCALL_DEFINE6_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_DEFINE6_sh,$(srctree))
