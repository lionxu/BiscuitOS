################################################
#
# SYSCALL_DEFINE2_common
#
################################################
PK_SYS_SYSCALL_DEFINE2_NAME        := SYSCALL_DEFINE2_common
PK_SYS_SYSCALL_DEFINE2_VERSION     := $(CONFIG_PK_SYS_SYSCALL_DEFINE2_VERSION)
PK_SYS_SYSCALL_DEFINE2_SITE        := $(CONFIG_PK_SYS_SYSCALL_DEFINE2_URL)
PK_SYS_SYSCALL_DEFINE2_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_DEFINE2_GITHUB)
PK_SYS_SYSCALL_DEFINE2_SUBNAME     :=
PK_SYS_SYSCALL_DEFINE2_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_DEFINE2_BUILDARCH)
PK_SYS_SYSCALL_DEFINE2_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_DEFINE2_common
PK_SYS_SYSCALL_DEFINE2_PATCH       := $(PK_SYS_SYSCALL_DEFINE2_DIR)/patch
PK_SYS_SYSCALL_DEFINE2_BSBIT       := SYSCALL_DEFINE2_common-$(PK_SYS_SYSCALL_DEFINE2_VERSION).bsbit
PK_SYS_SYSCALL_DEFINE2_OUT         :=
PK_SYS_SYSCALL_DEFINE2_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_DEFINE2_TAR         := $(CONFIG_PK_SYS_SYSCALL_DEFINE2_TAR)
PK_SYS_SYSCALL_DEFINE2_PACKAGE     :=
PK_SYS_SYSCALL_DEFINE2_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_DEFINE2_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_DEFINE2_DIR)/build

PK_SYS_SYSCALL_DEFINE2_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_DEFINE2_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_DEFINE2_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE2_VERSION)X \
                 $(PK_SYS_SYSCALL_DEFINE2_SITE)X \
                 $(PK_SYS_SYSCALL_DEFINE2_GITHUB)X \
                 $(PK_SYS_SYSCALL_DEFINE2_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE2_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_DEFINE2_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_DEFINE2_BSBIT)X \
                 $(PK_SYS_SYSCALL_DEFINE2_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_DEFINE2_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE2_CSRCFILE)

$(PK_SYS_SYSCALL_DEFINE2_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_DEFINE2_sh,$(srctree))
