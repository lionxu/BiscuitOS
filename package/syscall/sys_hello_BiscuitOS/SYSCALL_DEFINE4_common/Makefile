################################################
#
# SYSCALL_DEFINE4_common
#
################################################
PK_SYS_SYSCALL_DEFINE4_NAME        := SYSCALL_DEFINE4_common
PK_SYS_SYSCALL_DEFINE4_VERSION     := $(CONFIG_PK_SYS_SYSCALL_DEFINE4_VERSION)
PK_SYS_SYSCALL_DEFINE4_SITE        := $(CONFIG_PK_SYS_SYSCALL_DEFINE4_URL)
PK_SYS_SYSCALL_DEFINE4_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_DEFINE4_GITHUB)
PK_SYS_SYSCALL_DEFINE4_SUBNAME     :=
PK_SYS_SYSCALL_DEFINE4_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_DEFINE4_BUILDARCH)
PK_SYS_SYSCALL_DEFINE4_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_DEFINE4_common
PK_SYS_SYSCALL_DEFINE4_PATCH       := $(PK_SYS_SYSCALL_DEFINE4_DIR)/patch
PK_SYS_SYSCALL_DEFINE4_BSBIT       := SYSCALL_DEFINE4_common-$(PK_SYS_SYSCALL_DEFINE4_VERSION).bsbit
PK_SYS_SYSCALL_DEFINE4_OUT         :=
PK_SYS_SYSCALL_DEFINE4_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_DEFINE4_TAR         := $(CONFIG_PK_SYS_SYSCALL_DEFINE4_TAR)
PK_SYS_SYSCALL_DEFINE4_PACKAGE     :=
PK_SYS_SYSCALL_DEFINE4_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_DEFINE4_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_DEFINE4_DIR)/build

PK_SYS_SYSCALL_DEFINE4_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_DEFINE4_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_DEFINE4_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE4_VERSION)X \
                 $(PK_SYS_SYSCALL_DEFINE4_SITE)X \
                 $(PK_SYS_SYSCALL_DEFINE4_GITHUB)X \
                 $(PK_SYS_SYSCALL_DEFINE4_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE4_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_DEFINE4_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_DEFINE4_BSBIT)X \
                 $(PK_SYS_SYSCALL_DEFINE4_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_DEFINE4_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE4_CSRCFILE)

$(PK_SYS_SYSCALL_DEFINE4_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_DEFINE4_sh,$(srctree))
