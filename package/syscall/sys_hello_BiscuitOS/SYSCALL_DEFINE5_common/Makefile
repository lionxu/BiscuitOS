################################################
#
# SYSCALL_DEFINE5_common
#
################################################
PK_SYS_SYSCALL_DEFINE5_NAME        := SYSCALL_DEFINE5_common
PK_SYS_SYSCALL_DEFINE5_VERSION     := $(CONFIG_PK_SYS_SYSCALL_DEFINE5_VERSION)
PK_SYS_SYSCALL_DEFINE5_SITE        := $(CONFIG_PK_SYS_SYSCALL_DEFINE5_URL)
PK_SYS_SYSCALL_DEFINE5_GITHUB      := $(CONFIG_PK_SYS_SYSCALL_DEFINE5_GITHUB)
PK_SYS_SYSCALL_DEFINE5_SUBNAME     :=
PK_SYS_SYSCALL_DEFINE5_BUILDARCH   := $(CONFIG_PK_SYS_SYSCALL_DEFINE5_BUILDARCH)
PK_SYS_SYSCALL_DEFINE5_DIR         := $(srctree)/package/syscall/sys_hello_BiscuitOS/SYSCALL_DEFINE5_common
PK_SYS_SYSCALL_DEFINE5_PATCH       := $(PK_SYS_SYSCALL_DEFINE5_DIR)/patch
PK_SYS_SYSCALL_DEFINE5_BSBIT       := SYSCALL_DEFINE5_common-$(PK_SYS_SYSCALL_DEFINE5_VERSION).bsbit
PK_SYS_SYSCALL_DEFINE5_OUT         :=
PK_SYS_SYSCALL_DEFINE5_SCRIPTS     := $(srctree)/scripts/package/package-app.sh
PK_SYS_SYSCALL_DEFINE5_TAR         := $(CONFIG_PK_SYS_SYSCALL_DEFINE5_TAR)
PK_SYS_SYSCALL_DEFINE5_PACKAGE     :=
PK_SYS_SYSCALL_DEFINE5_CONFIG      := $(CONFIG_PK_SYS_SYSCALL_DEFINE5_CONFIG)
SUB_TARGET                  += $(PK_SYS_SYSCALL_DEFINE5_DIR)/build

PK_SYS_SYSCALL_DEFINE5_sh = $(CONFIG_SHELL) $(PK_SYS_SYSCALL_DEFINE5_SCRIPTS) \
                 $1X \
                 $(PK_SYS_SYSCALL_DEFINE5_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE5_VERSION)X \
                 $(PK_SYS_SYSCALL_DEFINE5_SITE)X \
                 $(PK_SYS_SYSCALL_DEFINE5_GITHUB)X \
                 $(PK_SYS_SYSCALL_DEFINE5_PATCH)X \
                 $(CONFIG_LINUX_KERNEL_VERSION)X \
	         $(CONFIG_MAIN_PROJ_NAME)X \
                 $(PK_SYS_SYSCALL_DEFINE5_TAR)X \
                 $(CONFIG_LINUX_KERNEL_CROSS_COMPILE)X \
                 $(PK_SYS_SYSCALL_DEFINE5_SUBNAME)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_CONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_LIBPATH)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_CPPFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_DPKCONFIG)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_CFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_LDFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_CXXFLAGS)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_ASFLAGS)X \
                 $(PK_SYS_SYSCALL_DEFINE5_BSBIT)X \
                 $(PK_SYS_SYSCALL_DEFINE5_BUILDARCH)X \
		 $(PK_SYS_SYSCALL_DEFINE5_CSRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_SRCFILE)X \
                 $(CONFIG_PK_SYS_SYSCALL_DEFINE5_CSRCFILE)

$(PK_SYS_SYSCALL_DEFINE5_DIR)/build: FORCE
	$(Q)$(call PK_SYS_SYSCALL_DEFINE5_sh,$(srctree))
